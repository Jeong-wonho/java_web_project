<insert id="insert" parameterType="hashmap" ><!-- RESULTTYPE이 필요없다. insert, update, delete  -->
INSERT INTO customer(id, pwd, name, buildingno, enabled)
VALUES (#{id}, #{pwd}, #{name}, #{buildingno}, 1)
</insert>
</mapper> 
 